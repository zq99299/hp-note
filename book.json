{
  "title": "hp-note",
  "description": "黄埔高端笔记",
  "author": "zhuqiang",
  "language": "zh-hans",
   "plugins": [
      "splitter",
      "-github",
      "katex",
      "prism",
      "-highlight",
      "expandable-chapters",
      "page-footer-ex",
      "anchor-navigation-ex",
      "advanced-emoji"
    ],
  "pluginsConfig": {
    "prism": {
       "css": [
         "prismjs/themes/prism-okaidia.css"
       ]
    },
    "anchor-navigation-ex":{
       "multipleH1":false,
       "printLog":true
    },
    "theme-default": {
       "showLevel": true
    },
    "page-footer-ex": {
         "copyright": "[朱强](https://github.com/zq99299/hp-note/blob/master/SUMMARY.md)",
         "markdown": true,
         "update_label": "<strong>updated</strong>",
         "update_format": "YYYY-MM-DD HH:mm:ss"
     }
  }
} 